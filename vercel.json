{
  "name": "researchhub",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "installCommand": "npm ci",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/client/$1"
    }
  ],  "env": {
    "NODE_ENV": "production",
    "PORT": "3002"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}
